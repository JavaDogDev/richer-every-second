{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "function-paren-newline": "off",
    "consistent-return": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "max-len": "off",
    "no-plusplus": "off",
    "no-prototype-builtins": "off"
  }
}
